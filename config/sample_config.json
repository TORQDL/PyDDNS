{
    "dns": [
        {
            "provider": "cloudflare",
            "authentication": {
                "api_token": "api_token_from_cloudflare",
                "api_key": "api_key_from_cloudflare",
                "account_email": "cloudflare_account_email"
            },
            "zones": [
                {
                    "zone_id": "zone_id_from_cloudflare_01",
                    "records": [
                        {
                            "domain": "example-cf.com",
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        },
                        {
                            "domain": "sub.example-cf.com",
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        }
                    ]
                },
                {
                    "zone_id": "zone_id_from_cloudflare_02",
                    "records": [
                        {
                            "domain": "example-two-cf.com",
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        },
                        {
                            "domain": "sub.example-two-cf.com",
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        }
                    ]
                }
            ]
        },
        {
            "provider": "dreamhost",
            "authentication": {
                "api_key": "api_key_from_dreamhost"
            },
            "records": [
                {
                    "domain": "example-dh.com",
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                },
                {
                    "domain": "sub.example-dh.com",
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                }
            ]
        },
        {
            "provider": "dreamhost",
            "authentication": {
                "api_key": "api_key_from_dreamhost"
            },
            "records": [
                {
                    "domain": "example-two-dh.com",
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                },
                {
                    "domain": "sub.example-two-dh.com",
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                }
            ]
        }
    ]
}