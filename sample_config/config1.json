{
    "dns": [
        {
            "provider": "cloudflare",
            "authentication": {
                "api_token": "api_token_from_cloudflare",
                "api_key": "api_key_from_cloudflare",
                "account_email": "cloudflare_account_email"
            },
            "zones": {
                "zone_id_01": {
                    "records": {
                        "zone1domain.com": {
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        },
                        "sub.zone1domain.com": {
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        }
                    }
                },
                "zone_id_02": {
                    "records": {
                        "zone2domain.com": {
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        },
                        "sub.zone2domain.com": {
                            "ipv4": true,
                            "ipv6": true,
                            "proxied": true,
                            "local_ip": false
                        }
                    }
                }
            }
        },
        {
            "provider": "dreamhost",
            "authentication": {
                "api_key": "api_key_from_dreamhost"
            },
            "records": {
                "example.com": {
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                },
                "sub.example.com": {
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                }
            }
        },
        {
            "provider": "dreamhost",
            "authentication": {
                "api_key": "api_key_from_dreamhost"
            },
            "records": {
                "example2.com": {
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                },
                "sub.example2.com": {
                    "ipv4": true,
                    "ipv6": true,
                    "local_ip": false
                }
            }
        }
    ]
}